{
  "expo": {
    "name": "TFM",
    "slug": "TFM",
    "scheme": "TFM",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/logo-tfm.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./src/assets/logo-tfm.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/logo-tfm.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.thiagospadarii.frontend"
    },
    "web": {
      "favicon": "./src/assets/logo-tfm.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ]
  }
}
